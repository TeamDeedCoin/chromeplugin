{
	"name": "Deedcoin Zillow Plugin",
	"version": "1.5",
	"description": "Find a house with Zillow, use Deedcoin to save on commissions!",
	"manifest_version": 2,
	"short_name": "Deedcoin",
	"author": "Deedcoin Inc",
	"incognito": "split",
	"permissions": [
		"cookies",
		"activeTab",
		"http://www.zillow.com/*",
		"https://www.zillow.com/*",
		"http://www.deedcoin.com/*",
		"https://www.deedcoin.com/*",
		"http://rpc.gochain.io/*",
		"https://rpc.gochain.io/*"
	],
	"icons": {
		"128": "icon.png",
		"32": "icon32x32.png",
		"16": "icon16x16.png"
	},
	"browser_action": {
		"default_title": "Deedcoin",
		"default_popup": "index.html"
	}
}